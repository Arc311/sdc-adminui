<td class="execution {{execution}}">
  <span class="label">
    {{#if succeeded}}<i class="icon-ok-sign"></i>{{/if}}
    {{#if failed}}<i class="icon-remove-sign"></i>{{/if}}
    {{execution}}
  </label>
</td>
<td>
  <a href="/jobs/{{uuid}}" class="name">{{name}} <small>{{summary}}</small></a>
  <div class="uuid">{{uuid}}</div>
</td>
<td>
  {{when}}<br>
  <span class="duration"><i class="icon-time"></i> {{duration}}s</span>
</td>
